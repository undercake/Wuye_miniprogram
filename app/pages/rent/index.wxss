page{
     height: 100%;
 }

.fixed{
    top: 0;
    left: 0;
    width: 100%;
    z-index: 10;
}
.page{
    height: 100%;
    background: #fff;
    overflow: scroll;
}
.header{
    background: #145aa1;
    z-index: 99;
}
.header view{
    color: #fff;
    z-index: 99;
}

.searchBar{
    padding: 0 30rpx;
    border-radius: 30rpx;
    background: #fff;
    height: 55rpx !important;
}

.searchBar input{
    width: 100%;
    text-align: left;
    color:#000;
    z-index: 99;
}

.nav{
    z-index: 99;
    display: flex;
    border-top: 1px solid #d1d3d4;
    border-bottom: 1px solid #d1d3d4;
    background: #fff;
}
.nav-son{
    display: flex;
    text-align: center;
    height: 40px;
    align-items:center;
    justify-content:center;
    font-size: 14px;
}
.borders{
    border-left: 1px solid #e6e6e6;
    border-right: 1px solid #e6e6e6;
}
.content{
    display: inline-block;
}
.icon{
    display: inline-block;
    border: 4px solid transparent;
    border-top: 4px solid #9b9b9b;
    margin-left: 5px;
}
.temp{
    position: relative;
    z-index: 4;
}
.temp1{
    display: none;
    width: 100%;
    max-height: 550rpx;
    overflow-y: scroll;
    padding: 0 0 0 20rpx;
    line-height:100rpx;
    background: #fff;
}
.slidedown{
    height:0;
}
.temp view{
    border-bottom: 1px solid #d1d3d4;
    font-size: 14px;
    color: #666;
}
.location{
    position: relative;
    display: none;
    float: left;
    width: 100%;
    height: 0;
    z-index: 5;
}
.location .qy{
    overflow-y: scroll;
    line-height: 80rpx;
    box-sizing: border-box;
    font-size: 14px;
    color: #717273;
}
.qy view{
    /* height: 100%; */
    text-overflow: ellipsis;
    overflow: hidden;
    overflow-y: scroll;
    white-space:nowrap;
    padding-left: 15rpx;
}

.qy{
    background-color: #fff;
    max-height:450rpx;
    /* height: 100%; */
}
.location-right view{
    border-bottom: 1px solid #c5c5c5;
}
.current{
    background: #e0e0e0;
}
.current2{
    background: #fff;
}
.bg{
    width: 100%;
    position: fixed;
    top:0;
    z-index: 1;
    background: rgb(1, 1, 1);
}
.zbg{
    height: 0;
    opacity: 0;
    transition: all .2s;
}
.fullbg{
    height: 100%;
    transition: all .01s
}

.blk{
    opacity: .5;
    transition: all .2s;
}

.header .active{

}
.nav-son.active .content{
    color: #61beff;
}
.nav-son.active .icon{
    border-bottom: 4px solid #61beff;
    border-top:0;
}
@keyframes sliup{
    from{
        height:450rpx;
    }
    to{
        height:0;
    }
}
.slidown{
    display: block;
    animation: slidown .2s ease-in both;
}
@keyframes slidown{
    from{
        height:0;
    }
    to{
        height:450rpx;
    }
}
.slidup{
    display: none;
    animation: slidup .2s ease-in both;
}

.list_title{
    padding-left: 30rpx;
}

.house_list{
    font-size: 24rpx;
}

.list_title view {
    font-size:24rpx;
    line-height: 30rpx;
    max-height: 30rpx;
    overflow: hidden;
    flex-wrap:nowrap;
    overflow: ellipsis;
}
